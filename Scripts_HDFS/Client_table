-- création d'une directory hadoop

$ hdfs dfs -mkdir /Clients_ESTIA2122MAZOUZI

-- ajout du fichier 
$ hdfs dfs -put $MYTPHOME/Clients/clients/Clients_12.csv /Clients_ESTIA2122MAZOUZI

--  Vérification de l'ajout.
$ hdfs dfs -ls /Clients_ESTIA2122MAZOUZI
