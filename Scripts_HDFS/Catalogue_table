-- création d'une directory hadoop

$ hdfs dfs -mkdir /Catalogue_ESTIA2122MAZOUZI

-- ajout du fichier 
$ hdfs dfs -put $MYTPHOME/hdfscatalogue/catalogue/Catalogue.csv /Catalogue_ESTIA2122MAZOUZI

--  Vérification de l'ajout.
$ hdfs dfs -ls /Catalogue_ESTIA2122MAZOUZI
